{
    "name": "apexmuse/laravel-dashboard-accuweather-tile",
    "description": "A tile for Spatie's Laravel Dashboard to display weather",
    "keywords": [
        "ApexMuse",
        "laravel-dashboard-accuweather-tile"
    ],
    "homepage": "https://github.com/ApexMuse/laravel-dashboard-accuweather-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Todd Twiggs",
            "email": "toddtwiggs@outlook.com",
            "homepage": "https://github.com/ApexMuse",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ApexMuse\\LaravelDashboardAccuweatherTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApexMuse\\MyTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApexMuse\\MyTile\\MyTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
